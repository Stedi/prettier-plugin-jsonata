{
  "name": "@stedi/prettier-plugin-jsonata",
  "version": "1.0.0",
  "description": "Prettier plugin for JSONata language",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "SKIP_EXPOSE=true jest --config ./test/jest.unit.ts",
    "types": "tsc -p tsconfig.build.json --noEmit",
    "lint": "eslint . --ext .ts",
    "style": "prettier -c .",
    "build": "npm run clean && npm run build:package",
    "build:package": "tsc -p tsconfig.build.json",
    "clean": "rm -rf ./dist",
    "release": "semantic-release",
    "prepare": "npm run build",
    "check": "concurrently npm:test npm:types npm:lint npm:style"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stedi/prettier-plugin-jsonata.git"
  },
  "keywords": [],
  "author": "transformers@stedi.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Stedi/prettier-plugin-jsonata/issues"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "homepage": "https://github.com/Stedi/prettier-plugin-jsonata#readme",
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@swc-node/jest": "^1.3.3",
    "@types/jest": "^27.0.2",
    "@types/prettier": "2.4.3",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "concurrently": "^6.3.0",
    "esbuild": "^0.13.9",
    "eslint": "^8.1.0",
    "eslint-plugin-jest": "^25.2.2",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "jest-extended": "1.2.0",
    "jest-matcher-specific-error": "1.0.0",
    "jsonata": "1.7.0",
    "prettier": "2.5.1",
    "semantic-release": "^18.0.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "jsonata": "^1.7.0",
    "prettier": "^2.5.1"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "volta": {
    "node": "14.16.0",
    "npm": "6.14.12"
  }
}
